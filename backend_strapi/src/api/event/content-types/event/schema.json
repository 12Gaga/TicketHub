{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "On_Live": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "ticket_limits": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket-limit.ticket-limit",
      "mappedBy": "event"
    }
  }
}
